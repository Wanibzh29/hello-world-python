      "source": [
        "def human_time(n_secs):\n",
        "  d = datetime(1,1,1) + timedelta(seconds=n_secs)\n",
        "  if d.day <= 1:\n",
        "    string = \"%d seconds (i.e. %02d:%02d:%02d) \" % (n_secs, d.hour, d.minute, d.second)\n",
        "  else:\n",
        "    string = \"%d seconds (i.e. %d-%02d:%02d:%02d) \" % (n_secs, d.day, d.hour, d.minute, d.second)\n",
        "  return string"
      ],
      "execution_count": 0,
      "outputs": []
    },
    {
      "cell_type": "code",
      "metadata": {
        "id": "3uuAqg1ijFhT",
